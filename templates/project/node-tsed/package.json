{
  "name": "{name}-tsed",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": "git@github.com:tomilenko/node-tsed.git",
  "author": "Vadym Tomilenko <tomilenko.vadim@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc",
    "coverage": "yarn test-cov && npm run test-remap",
    "postinstall": "yarn build",
    "tslint": "tslint ./*.ts ./lib/*.ts",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc -w",
    "start": "nodemon --watch '**/*.ts' --ignore 'node_modules/**/*' --exec ts-node src/index.ts"
  },
  "dependencies": {
    "@tsed/common": "^5.32.1",
    "@tsed/core": "^5.32.1",
    "@tsed/di": "^5.32.1",
    "@tsed/swagger": "^5.32.1",
    "@tsed/testing": "^5.32.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.2.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "tsc && yarn start"
  }
}