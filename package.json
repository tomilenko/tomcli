    "node-notifier": "^6.0.0",
    "nodegit": "^0.26.2",
    "path": "^0.12.7",
    "path-type": "^4.0.0",
    "string-template": "^1.0.0",
    "tslib": "^1",
    "underscore.string": "^3.3.5"
  },
  "devDependencies": {
    "@liftr/tscov": "^1.4.3",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^8.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/js-beautify": "^1.8.1",
    "@types/lodash": "^4.14.144",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-notifier": "^5.4.0",
    "@types/nodegit": "^0.24.11",
