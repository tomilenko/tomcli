{
  "name": "@tomilenko/tomcli",
  "version": "0.1.3",
  "author": "Vadym Tomilenko",
  "bin": {
    "tomcli": "./bin/run"
  },
  "bugs": "https://github.com/tomilenko/tomcli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@tomilenko/custom-template-generator": "^0.2.6",
    "@tomilenko/logger": "^0.0.4",
    "figlet": "^1.2.4",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
